{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/src",
                "${workspaceFolder}/lua-5.4.6/src"
            ],
            "defines": [
            ],
            "compilerArgs": [
                "-std=gnu99",
                "-Wall",
                "-Wextra",
                "-pedantic",
                "-Wstrict-prototypes",
                "-Wmissing-prototypes",
                "-Wno-unused-parameter",
                "-Wno-unused-function",
                "-O",
                "-g",
                "-c"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu99",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}/src",
                    "${workspaceFolder}/lua-5.4.6/src"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            }
        }
    ],
    "version": 4
}
