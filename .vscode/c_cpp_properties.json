{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "unix",
                "linux"
            ],
            "compilerArgs": [
                "-Wall",
                "-Wextra",
                "-std=c99",
                "-pedantic",
                "-Wstrict-prototypes",
                "-O",
                "-g"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c99",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64",
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}